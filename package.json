{
  "name": "eleventy-plugin-ignore",
  "version": "0.1.6",
  "description": "Ignore files based on their front matter.",
  "main": "lib/eleventy.js",
  "types": "lib/eleventy.d.ts",
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com:TheDocTrier/eleventy-plugin-ignore.git"
  },
  "author": "Michael Bradley <thedoctrier@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"**/*.ts\" \"**/*.json\"",
    "test": "jest --config=lib/jest.config.js",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test",
    "version": "yarn format"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "fast-glob": "^3.2.11",
    "gray-matter": "^4.0.3"
  }
}
